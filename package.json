{
  "name": "@davidlj95/website-v2",
  "version": "0.0.0-PLACEHOLDER",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run @davidlj95/website:serve-ssr",
    "serve:ssr": "node dist/@davidlj95/website/server/main.js",
    "build:ssr": "ng build && ng run @davidlj95/website:server",
    "prerender": "ng run @davidlj95/website:prerender",
    "render-templates": "ts-node scripts/src/generate-templated-files.ts",
    "security-txt": "ts-node scripts/src/generate-security-txt.ts",
    "font-subsets": "ts-node scripts/src/generate-font-subsets.ts",
    "lint": "ng lint",
    "git-hooks": "husky install",
    "commitlint-last": "commitlint --verbose --edit"
  },
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/platform-server": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/angular-fontawesome": "0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@ngaox/seo": "5.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "gardevoir": "^1.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@ngaox/devkit": "4.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "@types/subset-font": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.47.0",
    "eslint-formatter-multiple": "^2.0.0",
    "glob": "^10.3.3",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "liquidjs": "^10.8.4",
    "ng-mocks": "^14.11.0",
    "semantic-release": "^21.1.1",
    "subset-font": "^2.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "packageManager": "yarn@3.6.1",
  "eslint-formatter-multiple": {
    "formatters": [
      {
        "name": "stylish",
        "output": "console"
      },
      {
        "name": "junit",
        "output": "file",
        "path": "junit-lint.xml"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidlj95/website-v2.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
